<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <title>%{{title}}</title>
  <meta name="theme-color" content="#5468C0" media="(prefers-color-scheme: light)">
  <meta name="theme-color" content="#22377A" media="(prefers-color-scheme: dark)">
  <script src="/analytics.js"></script>
  @include-css stylesheet
  <script defer src="https://eluni.co/background-render.js"></script>
  <link rel="stylesheet" href="https://eluni.co/background-style.css">
</head>

<body>

  @include navbar


  <main id="main">
    %{{content}}
  <footer class="article-tags">
    %{{tags}}
  </footer>
    <section class="footer published">Published: <span data-has-date class="pub-date">%{{published}}</span></section>
  </main>

@include date_script


</body>

</html>
